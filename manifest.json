{
    "name": "BilibiliLiveSend",
    "version": "0.0.1",
    "description": "自动发送bilibili直播弹幕的chrome插件",
    "content_scripts": [{
        "css": [],
        "js": ["jquery.min.js","content_script.js"],
        "matches": ["http://live.bilibili.com/*"]
    }],
    "manifest_version": 2,
	"browser_action": {
      "default_icon": "image/48.png",
      "icon": {
         "128": "image/128.png",
         "16": "image/16.png",
         "48": "image/48.png"
      }
   }
}